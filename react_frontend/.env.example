# React Frontend (.env.example)
# Copy this file to .env and fill your Supabase credentials.

# Supabase project URL (Settings > API)
REACT_APP_SUPABASE_URL=https://YOUR-PROJECT.ref.supabase.co

# Supabase anon public key (Settings > API)
REACT_APP_SUPABASE_ANON_KEY=YOUR-ANON-PUBLIC-KEY

# Site URL used for auth redirects (match the dev server origin)
REACT_APP_SITE_URL=http://localhost:3000

# Backend base URL for proxy endpoints
REACT_APP_BACKEND_BASE_URL=http://localhost:8000

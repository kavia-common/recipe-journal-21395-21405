# React Frontend (.env.example)
# Copy this file to .env and fill the values for local development.
# CRA exposes variables prefixed with REACT_APP_ to the browser.

# Supabase (Frontend)
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_ANON_KEY=

# Site URL used for auth redirects (must end with a slash)
# For local dev, use: http://localhost:3000
REACT_APP_SITE_URL=http://localhost:3000

# Backend base URL for FastAPI proxy (no trailing slash)
# For local dev, use: http://localhost:8000
REACT_APP_BACKEND_BASE_URL=http://localhost:8000
